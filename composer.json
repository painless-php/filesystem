{
    "name": "nonetallt/filesystem",
    "description": "Filesystem manipulation functionality",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "nonetallt/string": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "spatie/phpunit-watcher": "^1.22",
        "vlucas/phpdotenv": "^4.1",
        "nonetallt/markdown-documentation": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nonetallt\\Filesystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.nanu.systems"
        }
    ]
}
